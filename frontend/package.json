{
  "name": "daigaku-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "tmsnvk",
  "description": "The application follows the university application process of high school students and handles student-mentor relationships.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tmsnvk/daigaku"
  },
  "keywords": [
    "react",
    "react-query",
    "react-hooks",
    "react-hook-form",
    "tailwind",
    "typescript",
    "zod"
  ],
  "main": "main.tsx",
  "scripts": {
    "dev:install": "npm install",
    "dev:install:clean": "npm ci",
    "dev:start": "vite",
    "lint:start": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,.jsx",
    "type-check:start": "tsc --noEmit",
    "test:install": "npx playwright install --with-deps chromium",
    "test:start": "npx playwright test",
    "build:start": "tsc && vite build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@hookform/resolvers": "5.1.1",
    "@tailwindcss/vite": "4.1.10",
    "@tanstack/react-query": "5.80.10",
    "@tanstack/react-router": "^1.125.1",
    "@tanstack/react-router-devtools": "^1.125.1",
    "@uidotdev/usehooks": "2.4.1",
    "axios": "1.10.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dotenv": "16.4.5",
    "i18next": "25.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.58.1",
    "react-i18next": "15.5.3",
    "tailwind-merge": "3.3.1",
    "ts-pattern": "5.7.1",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "1.53.1",
    "@tanstack/router-plugin": "^1.125.1",
    "@types/node": "22.15.32",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/parser": "8.34.1",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.13",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "vite": "6.3.5",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "5.1.4"
  }
